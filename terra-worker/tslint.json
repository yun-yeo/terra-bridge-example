{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-trailing-whitespace": [true, "always"],
    "prefer-const": [true, {"destructuring": "any"}],
    "semicolon": [true, "always"],
    "space-before-function-paren": [true, "never"],
    "quotemark": [true, "single"],
    "no-console": false,
    "no-string-literal": false
  },
  "rulesDirectory": []
}